package client;

import java.io.IOException;
import java.net.*;

public class Client {
	public static void main(String arg[]){
		try {
			Socket s = new Socket("127.0.0.1", 3000);
			System.out.println("Connected");
			
		} catch (IOException e) {
			System.out.println("There was a problem connecting to the Server");
			e.printStackTrace();
		}
	}
}
